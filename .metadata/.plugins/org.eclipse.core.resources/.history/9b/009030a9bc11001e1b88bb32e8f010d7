package com.example.example;

import javax.persistence.Entity;
import javax.persistence.Id;

@Entity 
public class Personel {
	int personelId;
	String personelAdi,personelSoyad,personelCinsiyet,personelPassword;

	@Id
	public int getPersonelId() {
		return personelId;
	}
	public void setPersonelId(int personelId) {
		this.personelId = personelId;
	}
	public String getPersonelAdi() {
		return personelAdi;
	}
	public void setPersonelAdi(String personelAdi) {
		this.personelAdi = personelAdi;
	}
	
	public String setPersonelSoyadi() {
		return personelSoyad;
	}
	public void setPersonelSoyad(String personelSoyad) {
		this.personelSoyad = personelSoyad;
	}
	public String getPersonelPassword() {
		return personelPassword;
	}
	public void setPersonelSoyadi(String personelPassword) {
		this.personelPassword = personelPassword;
	}
}
